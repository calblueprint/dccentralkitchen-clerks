{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "lint:js": "eslint . --ext  .jsx,.js ./screens ./components",
    "lint:js:fix": "yarn run lint:js --fix",
    "lint:js:quiet": "eslint . --ext .jsx,.js --quiet ./screens ./components",
    "lint:js:quiet:fix": "yarn run lint:js:quiet --fix",
    "pretty": "prettier --config prettier.config.js --write \"./{screens,components,styles}/**/*.js\" \"./{screens,components,styles}/*.js\"",
    "generate-schema": "generate-airtable-schema"
  },
  "airtable-schema-generator": {
    "input": "lib/airtable",
    "output": "lib/airtable"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --config prettier.config.js --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^10.0.3",
    "@react-navigation/core": "^3.0.0-alpha.13",
    "@react-navigation/stack": "^5.2.4",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "airtable": "^0.7.2",
    "airtable-schema-generator": "^1.2.9",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "expo": "^35.0.0",
    "expo-asset": "^7.0.0",
    "expo-constants": "^7.0.0",
    "expo-font": "^7.0.0",
    "expo-web-browser": "^7.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mem": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-addons-update": "^15.6.2",
    "react-dom": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-alert-async": "^1.0.5",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-web": "^0.11.7",
    "react-navigation": "^3.12.0",
    "styled-components": "^4.4.1",
    "yarnhook": "^0.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jest-expo": "^35.0.0",
    "prettier": "^1.19.1"
  },
  "private": true
}
